void SystemStateHandler::vSetGreenLED(bool on){

	HAL_GPIO_WritePin (GPIOB,  GPIO_Pin_0, GPIO_PIN_SET);

}
void SystemStateHandler::vSetRedLED(bool on){

	HAL_GPIO_WritePin (GPIOB,  GPIO_Pin_0, GPIO_PIN_SET);

}
void SystemStateHandler::vSetBlueLED(bool on){

	HAL_GPIO_WritePin (GPIOB,  GPIO_Pin_0, GPIO_PIN_SET);

}

